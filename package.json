{
  "name": "paperstation-browser",
  "version": "1.1.5",
  "description": "访问互联网",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "cross-env NODE_ENV=development electron .",
    "build": "electron-builder --win --x64",
    "build:dir": "electron-builder --win --x64 --dir",
    "dist": "electron-builder --win --x64 --publish never",
    "pack": "electron-builder --dir"
  },
  "build": {
    "appId": "com.paperstation.browser",
    "productName": "PaperStation",
    "copyright": "Copyright © 2026 Ruanm",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.png",
      "artifactName": "${productName}_Win64_${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "PaperStation"
    },
    "asar": true,
    "compression": "maximum"
  },
  "keywords": [
    "browser",
    "electron",
    "chromium",
    "paperstation"
  ],
  "author": "RuanMingze",
  "license": "MIT",
  "dependencies": {
    "electron-context-menu": "^4.1.1",
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "electron": "^40.2.1",
    "cross-env": "^7.0.3",
    "electron-builder": "^25.1.8"
  }
}